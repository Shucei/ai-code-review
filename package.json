{
  "name": "ai-code-review",
  "version": "1.0.0",
  "description": "AI驱动的GitLab代码审查工具，支持自动检测MR并推送审查评论",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "review:ci": "ts-node src/ci.ts",
    "lint": "eslint --ext .ts src",
    "test": "jest",
    "test:webhook": "node test-webhook.js",
    "test:ai": "node test-ai-review.js",
    "test:gitlab": "node test-gitlab-api.js",
    "test:all": "npm run test:gitlab && npm run test:ai && npm run test:webhook"
  },
  "keywords": [
    "code-review",
    "gitlab",
    "ai",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "openai": "^4.28.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "packageManager": "pnpm@10.18.3+sha512.bbd16e6d7286fd7e01f6b3c0b3c932cda2965c06a908328f74663f10a9aea51f1129eea615134bf992831b009eabe167ecb7008b597f40ff9bc75946aadfb08d"
}
